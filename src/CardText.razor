@using Soenneker.Quark.Paragraphs
@inherits Soenneker.Quark.Components.Element

<Paragraph @attributes="BuildAttributes()">
    @ChildContent
</Paragraph>

@code {
    protected override Dictionary<string, object> BuildAttributes()
    {
        Dictionary<string, object> attributes = base.BuildAttributes();
        const string classes = "card-text";
        AppendToClassAttr(attributes, classes);
        return attributes;
    }
}


